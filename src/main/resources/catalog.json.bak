                      "options": [
                        2,
                        3,
                        4
                      ]
                    },
                    {
                      "variable": "version",
                      "label": "版本号",
                      "description": "mysql 版本号",
                      "default": 5.7,
                      "type": "enum",
                      "options": [
                        5.6,
